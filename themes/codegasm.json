    {
      "scope": ["comment", "punctuation.definition.comment", "source.diff"],
      "settings": {
        "foreground": "#636369"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "entity.name.function.member",
        "support.function",
        "meta.diff.range",
        "punctuation.definition.range.diff"
      ],
      "settings": {
        "foreground": "#af85ff"
      }
    },
    {
      "scope": [
        "keyword",
        "punctuation.definition.keyword",
        "variable.language",
        "markup.deleted",
        "meta.diff.header.from-file",
        "punctuation.definition.deleted",
        "punctuation.definition.from-file.diff"
      ],
      "settings": {
        "foreground": "#f44081"
      }
    },
    {
      "scope": ["constant", "support.constant"],
      "settings": {
        "foreground": "#d65fd2"
      }
    },
    {
      "scope": [
        "storage",
        "support.class",
        "entity.name.namespace",
        "meta.diff.header"
      ],
      "settings": {
        "foreground": "#6fb4eb"
      }
    },
    {
      "scope": [
        "markup.inline.raw.string",
        "string",
        "markup.inserted",
        "punctuation.definition.inserted",
        "meta.diff.header.to-file",
        "punctuation.definition.to-file.diff"
      ],
      "settings": {
        "foreground": "#fce564"
      }
    },
    {
      "scope": [
        "entity.name.section",
        "entity.name.tag",
        "entity.name.type",
        "support.type"
      ],
      "settings": {
        "foreground": "#5fc976"
      }
    },
    {
      "scope": ["support.type.property-name", "support.variable", "variable"],
      "settings": {
        "foreground": "#c6c7ca"
      }
    },
    {
      "scope": [
        "entity.other",
        "punctuation.definition.entity",
        "support.other"
      ],
      "settings": {
        "foreground": "#f59762"
      }
    },
    {
      "scope": ["meta.brace", "punctuation"],
      "settings": {
        "foreground": "#c6c7ca"
      }
    },
    {
      "name": "bold",
      "scope": [
        "markup.bold",
        "punctuation.definition.bold",
        "entity.other.attribute-name.id"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "italic",
      "scope": ["comment", "markup.italic", "punctuation.definition.italic"],
      "settings": {
        "fontStyle": "italic"
      }
    }
  ]
