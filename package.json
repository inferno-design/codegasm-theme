{
  "publisher": "InfernoDesign",
  "name": "codegasm-theme",
  "displayName": "Codegasm Theme",
  "description": "A dark theme for Visual Studio Code.",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/inferno-design/codegasm-theme.git"
  },
  "bugs": {
    "url": "https://github.com/inferno-design/codegasm-theme/issues"
  },
  "keywords": [
    "theme",
    "dark",
    "codegasm"
  ],
  "categories": [
    "Themes"
  ],
  "maintainers": [
    {
      "name": "Inferno Design",
      "email": "support@infernodesign.com"
    }
  ],
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.97.0"
  },
  "packageManager": "pnpm@10.4.1",
  "contributes": {
    "themes": [
      {
        "label": "Codegasm",
        "uiTheme": "vs-dark",
        "path": "./themes/codegasm.json"
      }
    ]
  },
  "galleryBanner": {
    "color": "#5c16c5",
    "theme": "dark"
  },
  "scripts": {
    "build": "esno src/index.ts",
    "dev": "esno watch src/index.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "package": "vsce package --no-dependencies --out ./releases/ --no-yarn",
    "prepublishOnly": "npm run build",
    "publish": "bumpp --all && vsce publish --no-dependencies",
    "vscode:prepublish": "npm run build"
  },
  "devDependencies": {
    "@commitlint/cli": "19.7.1",
    "@commitlint/config-conventional": "19.7.1",
    "@commitlint/format": "19.5.0",
    "@commitlint/types": "19.5.0",
    "@inferno-design/eslint-config": "2.2.1",
    "@inferno-design/typescript-config": "1.4.0",
    "@types/fs-extra": "11.0.4",
    "@types/node": "22.13.4",
    "@types/tinycolor2": "1.4.6",
    "@types/vscode": "1.97.0",
    "@vscode/vsce": "3.2.2",
    "bumpp": "10.0.3",
    "eslint": "9.20.1",
    "esno": "4.8.0",
    "fs-extra": "11.3.0",
    "ovsx": "0.10.1",
    "prettier": "3.5.1",
    "tinycolor2": "1.6.0",
    "typescript": "5.7.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@vscode/vsce-sign"
    ]
  }
}
